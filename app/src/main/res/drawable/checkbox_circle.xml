<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <inset android:drawable="@drawable/ic_radio_uncheck" android:insetRight="@dimen/_12dp" />
    </item>
    <item android:state_checked="true">
        <inset android:drawable="@drawable/ic_radio_checked" android:insetRight="@dimen/_12dp" />
    </item>
    <item>
        <inset android:drawable="@drawable/ic_radio_uncheck" android:insetRight="@dimen/_12dp" />
    </item>
</selector>